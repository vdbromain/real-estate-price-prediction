    "\n",
    "import pickle\n",
    "import numpy as np\n",
    "\n",
    "model = pickle.load(open(\"../model/model.pkl\", 'rb'))\n",
    "\n",
    "def predict(prepro_data : list) -> float :\n",
    "    features = [np.array(prepro_data)]\n",
    "    predicted_price = model.predict(features)\n",
    "\n",
    "\n",
    "    return predicted_price "
   ]
